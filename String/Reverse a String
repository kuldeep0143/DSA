class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        String nstr="";
       char ch;
       for(int i=0;i<str.length();i++){                           //TC :- O(n) 
           ch=str.charAt(i);                                      //SC :- O(1)
           nstr=ch+nstr;
       }
       return nstr;
    }
}
